<script src="@Url.Content("~/Scripts/jquery.sticky.js")"></script>
    <script type="text/javascript">
        var refreshIntervalId = 0;
        $(document).ready(function() {
            //$('.newsletter').hide();
            $('.newsletter').sticky({topSpacing: 40});
            $('.newsletter  *').css('box-sizing', 'content-box');
            var FF = !(window.mozInnerScreenX == null);
            if(FF) {
                // is firefox 
                $('.newsletter  input').css('height', '26px');
            } 
           
            // newsletter sign up slide in box
            $('.newsletter-slide-btn').click(function() {
                if($(this).hasClass("isOpen")) {  
                    $(this).removeClass("isOpen ").show('slow');
                    $('.newsletter').animate({right:'-230px'});
                    $('#newsletter-boxContent').addClass('newsletter-boxContent');
                } else {
                    $(this).addClass("isOpen").hide('slow');
                    $('.newsletter').animate({right:'0px'});
                    $('#newsletter-boxContent').removeClass('newsletter-boxContent');   
                }
            });
            $('.newsletter-close-btn').click(function(event) {
                $('.newsletter-slide-btn').click();
            });

            $('#newsletter-box').submit(function(event) {
                event.preventDefault();
                // add form validation and ajax submit here (see what Swati has on site)
                // newsletter validation
                $('#newsletter-box').validate({
                    onkeyup: true      
                });
                if ($('#newsletterboxemail').valid()) {
                  
                    Subscribe(true);
                    $('#newsletter-boxContent').hide('slow');
                    $('#newsletter-confirmed').show().delay(3000).hide('slow');
                    $('.newsletter').delay(3300).animate({right:'-230px'});
                    $('#newsletter-boxContent').delay(3200).show('100');
                    $('.newsletter-slide-btn').delay(3200).show('100');
                    $('#newsletter-boxContent').addClass('newsletter-boxContent');
                    console.log($("#newsletterboxemail").val());
                    $("#newsletterboxemail").val('');
                    $("#newsletterboxemail").attr("placeholder", "");
                } else {// do nothing for now?
                }
            });
            
            // press release class fix
            $('.item').first().addClass("active");
            //date stuff
            $('#reportrange').val("");

            $.datepicker.setDefaults($.datepicker.regional['en-GB']);
            $.datepicker.setDefaults({ dateFormat: 'dd/mm/yy' });

            $('.date').datepicker({
                dateFormat: "dd/mm/yy",
                minDate: new Date(@DateTime.Now.Year, @DateTime.Now.Month - 1, @DateTime.Now.Day)
            });

            $('#reportrange').daterangepicker({
                ranges: {
                    'Today': [moment(), moment().add('days', 0)],
                    'This Week': [moment(), moment().endOf('week')],
                    'This Month': [moment().startOf('month'), moment().endOf('month')],
                    'All': [moment(), moment().add('days', 365)]
                },
                format: 'DD/MM/YYYY',
                minDate: moment(),
                startDate: moment(),
                endDate: moment().add('days', 15)
            },
            function(start, end) {
                $('#DateString').val(start.format('DD MMM YYYY') + ' - ' + end.format('DD MMM YYYY'));
                //$('#hdnDateString').val(start.format('DD/MM/YYYY') + '|' + end.format('DD/MM/YYYY'));
            }
           );


            $('div.alt').click(function () {

                scrollToElement('#content', 1500);

            })

            $('#reportrange').click(function () {
                if ($(window).width() <= 768){
                    scrollToElement('#reportrange', 1500);
                }
            })

            $('#btnSignup').click(function(){
                document.location.href='http://grubclub.com/grub-club-gift-voucher-grub-club-gift-voucher/777';
                return false;
            })
            $('#btnGnocchi').click(function(){
                document.location.href='http://grubclub.com/quintoquarto';
                return false;
            })

            // Modal form
            // some user feedback before form submit for number only input
            $('input[type="number"]').keyup(function() {
                this.value = this.value.replace(/[^0-9\.]/g,'');
            });
            // datepicker
            $( "#dateChoice" ).datepicker({
                dateFormat: "dd-mm-yy",
                minDate: new Date(@DateTime.Now.Year, @DateTime.Now.Month - 1, @DateTime.Now.Day)
            });
            $("#dateChoice").datepicker({
                onClose: function() {
                    $( this ).valid();
                }
            });
            $("input.required").closest(".form-group").find("label").append("<i style='color: Red;float:right'> *</i> ");
            //validation rules

            var validator =  $('#privategcform').validate({
                onkeyup: true,
                rules: {
                    yourEmail: {
                        required: true,
                        minlength: 3,
                        email:true
                    },
                    noPeople: {
                        required: true,
                        digits: true,
                        minlength: 1,
                        min: 1
                    },
                    pricePoint: {
                        required: true,
                        digits: true,
                        minlength: 1,
                        min: 1
                    },
                    cuisineChoice: {
                        required: true,
                        text: true,
                        maxlength: 20
                    },
                    //location: {
                    //    text: true,
                    //    requiRed: true,
                    //    maxlength: 40
                    //},
                    //other: {
                    //    text: true,
                    //    requiRed: true,
                    //    maxlength: 250
                    //},
                    dateChoice: {
                        dateISO: true,
                        required: true
                    }
                },
                messages: {
                    yourEmail: {
                        required: "<h4>We require your email address so that we can contact you</h4>",
                        email: 'Please you provide a some valid email address'
                    },
                    noPeople: {
                        required: 'We require an indication of the number of people to be catered for',
                        digits: 'Please only enter numbers'
                    },
                    pricePoint: {
                        required: 'We require your budget in order to plan your event',
                        digits: 'Please only enter numbers',
                    },
                    location: {
                        required: "We need to know if you have a location planned; if not then enter 'unplanned'",
                    }
                },
                tooltip_options: {
                    yourEmail: {html:true},
                    noPeople: {html:true},
                    pricePoint: {html:true}
                },
                errorClass: "input-validation-error",
                validClass: "valid",
                highlight: function(element, errorClass, validClass) {
                    $(element).addClass(errorClass).removeClass(validClass);
                },
                unhighlight: function(element, errorClass, validClass) {
                    $(element).removeClass(errorClass).addClass(validClass);
                }//,
                //submitHandler: function(form) {
                //    privateRequest();
                //}
            });

            $('#privategcform').submit(function(e){ e.preventDefault(); if ($('#privategcform').valid()) {privateRequest(); }});

            $('#btnPrivateSC').click(function() {
                if ($('#privategcform').valid()) {
                    privateRequest();
                }
                return false;
            });
            $('#successtick').hide();
            $('#privategcmodal').on('hidden.bs.modal', function () {
                $('#myModalLabel').html('Please let us know the following');
                $('#myModalLabel').css('color', '#700');
                $('#privsuccess').hide();
                $('#successtick').hide();
                var form = $('#privategcform');
                form.show();
                validator.resetForm();
                form[0].reset();
            });
        });

        //end modal stuff
        function privateRequest(){
            var emailAddress = $('#yourEmail').val();
            var numOfguests = $('#noPeople').val();
            var cuisineChoice = $('#cuisineChoice').val();
            var pricePerHead = $('#pricePoint').val();
            var dateChoice = $('#dateChoice').val();
            var locationChoice = $('#locationChoice').val();
            var otherRequirements = $('#otherReq').val();
            var form = $('#privategcform');
            $.ajax({
                type: 'POST',
                url: '@Url.Action("privategrubclubrequest","home")',
                data: {email : emailAddress, numberOfGuests : numOfguests, cuisine : cuisineChoice, pricePerHead : pricePerHead, dateChoice : dateChoice, locationChoice : locationChoice, otherRequirements : otherRequirements},
                cache: false,
                dataType: 'json text',
                traditional: true,
                success: function (result) {
                    if(result)
                    {
                        $('#privategcform').hide();
                        $('#successtick').show();
                        $('#myModalLabel').html('<span id="successtick"><img style="width: 30px; height: 30px; display: inline;" src="@Url.Content("~/Content/images/successtick.png")" alt=""/></span> Submission Successful!');
                        $('#myModalLabel').addClass('text-success');
                        $('#privategcmodal-body').append('<p id="privsuccess" style="margin:auto;text-align:center">Thank you for submitting your request. We’ll look into this and come back to you as soon as possible to see how best we can help! </p>');

                        form[0].reset();
                        validator.resetForm();
                        return true;
                    }
                    else
                        alert('Sorry, there was an error submitting your request. Please try again!');
                },
                error: function (result) {
                    alert('Sorry, there was an error submitting your request. Please try again!');
                }
            });
        }; //end privategc stuff


        // simple code sourced for scroll points.
        function scrollToElement(selector, time, verticalOffset) {
            time = typeof(time) != 'undefined' ? time : 2000;
            verticalOffset = typeof(verticalOffset) != 'undefined' ? verticalOffset : -50;
            element = $(selector);
            offset = element.offset();
            offsetTop = offset.top + verticalOffset;
            $('html, body').animate({
                scrollTop: offsetTop
            }, time, 'easeInCubic');
        };

        function Subscribe(newsletterbox){
            
            var SubscribeEmailAddress = newsletterbox ? $('#newsletterboxemail').val() : $('#SubscribeEmailAddress').val();
           
            if(SubscribeEmailAddress != '' && SubscribeEmailAddress != null){
                $.ajax({
                    type: "POST",
                    url: '@Url.Action("subscribe","home")',
                    data: { emailAddress: SubscribeEmailAddress },
                    cache: false,
                    dataType: 'json text',
                    traditional: true,
                    success: function (result) {
                        if(result == "true")
                        {
                            $('#SubscribeEmailAddress').hide();
                            $('#btnSubscribe').hide();
                            $('#txtSubscribeError').hide();

                            $('#hdrSubscribe').text('Welcome to the Club!');
                            $('#txtSubscribe').text("Thanks for subscribing. We'll let you know about any new events and any other news!");
                        }
                        else
                        {
                            $('#txtSubscribeError').text("Looks like there is a problem with your email address, please try again!");
                            $('#txtSubscribeError').show();
                        }
                    },
                    error: function () {
                        alert("Could not save email address.");
                    }
                });
            } else { alert('There was an error with the submission - please try again.');};
        };

        $(function () {
            $('.fadein .activeSlide:gt(0)').hide();
            refreshIntervalId = setInterval(function () { $('.fadein .activeSlide:eq(0)').fadeOut().next('.activeSlide').fadeIn().end().appendTo('.fadein'); }, 5000);
        });

        function LatestEventSliderStop()
        {
            clearInterval(refreshIntervalId);
        };

        function LatestEventsSlider()
        {
            refreshIntervalId = setInterval(function () { $('.fadein .activeSlide:eq(0)').fadeOut().next('.activeSlide').fadeIn().end().appendTo('.fadein'); }, 5000);
        };
    </script>
}
