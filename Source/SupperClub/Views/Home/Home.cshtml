                        <div class="justifize__box">
                            <a href="/${GrubClubUrlFriendlyName}/#reviews">
                                <span class="color-gray">
                                    <svg role="img" class="icon ">
                                        <use xlink:href="@Url.Content("~/Content/images/")icons.svg#icon-bubble_icon"></use>
                                    </svg>
                                    ${ReviewCount} Reviews
                                </span>
                            </a>
                        </div>
                    </div>
                </header>
                <footer>
                    <div class="tableize tableize--bottom">
                        <div class="tableize__cell">
                            <div class="justifize">
                                <div class="justifize__box">
                                    <span class="price">
                                        {{if Cost == "0.00"}}
                                        Free
                                        {{else}}

                                        £${Cost}
                                        {{/if}}
                                    </span>
                                </div>
                                <div class="justifize__box">
                                    <time>
                                        <svg role="img" class="icon ">
                                            <use xlink:href="@Url.Content("~/Content/images/")icons.svg#icon-small_calendar"></use>
                                        </svg>
                                        ${EventDate}
                                    </time>
                                </div>
                            </div>
                        </div>
                    </div>
                </footer>

                <div>
                    {{if WishEvent == 0}}
                    @if (SupperClub.Code.UserMethods.IsLoggedIn)
                    {
                        <div class="tooltip-links-wl is-active hidden" id="divRmWs_${EventId}">
                            <a href="#!" onclick="_removeWS(${EventId});">
                                <svg role="img" class="icon color-white">
                                    <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="../content/images/icons/icons.svg#icon-heart"></use>
                                </svg>
                            </a>
                        </div>
                        <div class="tooltip-links-wl" id="divAddWs_${EventId}">
                            <a href="#!" onclick="_addWS(${EventId});">
                                <svg role="img" class="icon color-white">
                                    <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="../content/images/icons/icons.svg#icon-heart"></use>
                                </svg>
                            </a>
                        </div>

                    }
                    else
                    {
                        <div class="tooltip-links-wl">
                            <span class="loginWishList open-popup" id="${EventId} _tooltip" onclick="wishlist_login(this)">
                                <svg role="img" class="icon color-white">
                                    <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="../content/images/icons/icons.svg#icon-heart"></use>
                                </svg>
                            </span>
                        </div>

                    }
                    {{/if}}
                    {{if WishEvent == 1}}
                    <div class="tooltip-links-wl is-active" id="divRmWs_${EventId}">
                        <a href="#!" onclick="_removeWS(${EventId});">
                            <svg role="img" class="icon color-white">
                                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="../content/images/icons/icons.svg#icon-heart"></use>
                            </svg>
                        </a>
                    </div>
                    <div class="tooltip-links-wl hidden" id="divAddWs_${EventId}">
                        <a href="#!" onclick="_addWS(${EventId});">
                            <svg role="img" class="icon color-white">
                                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="../content/images/icons/icons.svg#icon-heart"></use>
                            </svg>
                        </a>
                    </div>
                    {{/if}}


                    @*<ul class="list-bare">
                            <li><h3 class="type-small">New list title here...</h3></li>
                            <li><a href="">Indian Food</a></li>
                            <li><a href="">Interesting Places</a></li>
                            <li><a href="">Valentines &lt; 3</a></li>
                        </ul>*@
                </div>

            </div>
        </div>
    </div>
</script>

