    ViewBag.Title = "Pop Up Dining Experiences in London | Book with Grub Club";
    ViewBag.Description = "Grub Club curates the best Pop Ups and Supper Clubs in London. Try exciting menus from hundreds of Top Chefs while dining in Unique Spaces.";
}
@section PageSpecificScripts {
   
    <style type="text/css">

        .hero {
            background-image: url("@ViewBag.Hero[0].ImagePath");
            background-position: 50% 50%;
            background-size: cover;
            position: relative;
        }

             .hero:after {
                content: "";
                position: absolute;
                top: 0;
                bottom: 0;
                left: 0;
                right: 0;
                z-index: 100;
            }

           .hero > * {
                position: relative;
                z-index: 200;
            }

        .hero__bottom {
            background-color: white;
            background-color: rgba(255, 255, 255, 0.6);
        }
    </style>
    <script src="@Url.Content("~/Scripts/jquery.tmpl.js")" type="text/javascript"></script>

    <script type="text/javascript">

        var pageNumber = 1;
        var pageSize = 8;
        var isInt = false;
        var wishEvent;

        function _addWishList(SupperClubId) {


            $.ajax({
                type: "POST",
                url: '@Url.Action("AddEventToFavourite", "Event")',
                contentType: "application/json",
                dataType: "json",

                data: JSON.stringify({eventId: SupperClubId}),
                success: function(response) {
                    // alert(response.messagebehind
                    $(".message-popup-wraper .messages-popup").addClass("is-hidden"), $(".messages-popup--added").clone().appendTo(".message-popup-wraper").addClass("is-active").delay(3100).queue(function () { $(this).remove() }), $(".messages-popup").on("click", function () { $(this).addClass("is-hidden") });

                },
                error: function(response) {
                    alert(response.status);
                }
            });

        }
        function _removeWishList(SupperClubId) {

            $.ajax({
                type: "POST",
                url: '@Url.Action("RemoveEventFromFavourite", "Event")',
                contentType: "application/json",
                dataType: "json",

                data: JSON.stringify({eventId: SupperClubId}),
                success: function(response) {
                    //  alert(response.message);
                    $(".messages-popup--removed").clone().appendTo(".message-popup-wraper").addClass("is-active").delay(3100).queue(function () {
                        $(this).remove()
                    }),
                  $(".messages-popup").on("click", function () { $(this).addClass("is-hidden") });
                },
                error: function(response) {
                    alert(response.status);
                }
            });
        }

        function _addWS(eventId)
        {
            _addWishList(eventId);
            $("#divRmWs_"+ eventId.toString()).removeClass('hidden');
            $("#divAddWs_" + eventId.toString()).addClass('hidden');
            return false;
        }

        function _removeWS(eventId)
        {
            _removeWishList(eventId);
            $("#divRmWs_"+ eventId.toString()).addClass('hidden');
            $("#divAddWs_" + eventId.toString()).removeClass('hidden');
            return false;
        }

        function  wishlist_login(e)
        {
            wishEvent= e.id.split(" ")[0];
            $("#commonLogin").click();
        }

        function ForceReloadInit(){
            $.ajax({
                url: "@Url.Action("Home_Init", "Home")",
                type: "POST",
            data : {
                forceReload : 'true' 
            },
            dataType: "json",
            traditional: true,
            success: function (result) {
                renderResults(result);
            },

            error: function () {
                        
            }});
        }

        $(document).ready(function () {



            $('#PageNo').val("1");
            if (isInt) return;
            isInt = true;
            // showLoadMore();
            debugger;
            $.ajax({
                url: "@Url.Action("Home_Init", "Home")",
                type: "POST",
                dataType: "json",
                traditional: true,
                success: function (result) {

                    renderResults(result);
                },

                error: function () {
                    ForceReloadInit();
                }

            });
            $('#SearchCriteriaForm').submit(function () {

                var KeyWord = $("#txtKeyWord").val();
                if (KeyWord != null && KeyWord != "") {
                    window.location = '/pop-up-restaurants?keyword=' + KeyWord;
                }
                return false;
            });
            $("#btnSearch").click(function () {

                var KeyWord = $("#txtKeyWord").val();
                if (KeyWord != null && KeyWord != "") {
                    window.location = '/pop-up-restaurants?source=2&keyword=' + KeyWord;
                }

            });

            $("#btnLoadMore").click(function () {

                renderPagedResults();

            });

            function renderResults(result) {
                pageNumber = 1;
                $('#ResultCount').val(result.length)
                $("#List-results1").empty(); 		 // Remove current
                if (result.length > 0) {

                    //$("#eventsListTemplate1").tmpl(result.SearchResults).appendTo("#List-results1");
                    //showLoadMore();

                    //$("#totalResult").text(" ("+result.SearchResults.length+")");
                    results = result;//.SearchResults;

                    renderPagedResults();

                }

            }

            function renderPagedResults() {
                var currPage = pageNumber;
                var startIndex = (currPage - 1) * pageSize;
                var targetNum = (currPage) * pageSize;
                var toBeRenderedresult = [];
                for (var i = startIndex; i < targetNum && i < results.length; i++) {
                    toBeRenderedresult.push(results[i]);
                }
                $('#eventsListTemplate1').tmpl(toBeRenderedresult).appendTo('#List-results1');

                showLoadMore();
                ++pageNumber;
            }

            $(".loginWishList").click(function (e) {
                if(wishEvent!= null)
                {
                    e.preventDefault();
                    //var clkId = jQuery(this).attr("id");
                    //var eId = parseInt(clkId.split(" ")[0]);

                    var _rurl = "@(SupperClub.Code.ServerMethods.ServerUrl)";
                    var tem = "@Url.Action("WishListEvent", "event")";
                    var _url = encodeURIComponent(tem + "?eventId=" + wishEvent.toString() + "&rurl=" + _rurl);
                    $("#RedirectURL").val(_url); $('#login-dialog').load("@Url.Action("loginRegister", "account", new {})"+ "?returnUrl=" + _url);
                    $( $(this).attr("href")).show();

                    return false;
                }
            });


            function showLoadMore() {

                var count = $('#ResultCount').val();
                var page = pageNumber;

                if (count > page * pageSize) {

                    $("#btnLoadMorediv").css("display", "block");
                }
                else {

                    $("#btnLoadMorediv").css("display", "none");
                }

            }

        });

    </script>

