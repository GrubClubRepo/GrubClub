    <script type="text/javascript">
        $(document).ready(function () {

            $("#noPeople").keydown(function (e) {
                // Allow: backspace, delete, tab, escape, enter and .
                if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 110, 190]) !== -1 ||
                    // Allow: Ctrl+A
                    (e.keyCode == 65 && e.ctrlKey === true) ||
                    // Allow: home, end, left, right
                    (e.keyCode >= 35 && e.keyCode <= 39)) {
                    // let it happen, don't do anything
                    return;
                }
                // Ensure that it is a number and stop the keypress
                if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
                    e.preventDefault();
                }
            });

      	//set up the full page scroll effect plugin
      	$('#fullpage').fullpage({
      		verticalCentred: true,
      		autoScrolling: false,
      		resize: false,
      		anchors: ['about', 'in-the-chefs-place', 'at-your-place'],
      		css3: true,
      		menu: '#top-nav'
      	});

       	// Adjust the text content of button(s) for small screens
       	$( window ).resize(function() {
       		if ($(window).width()<650) {
       			$('#showMeChefs').text('Show Me');
       		} else {
       			$('#showMeChefs').text('Show Me The Chefs');
       		}
       			// reload the slider for responsiveness bug-fixing
       			slider.reloadSlider();
       	});


       var slider =	$('.bxslider').bxSlider({
       		auto: true,
       		responsive: false,
       		controls: false
       	});


       // setting up the modal and form contents
       	$('input.required').parent().siblings('.col-sm-4').children('label').addClass('required');
        var min = null;
        var max = null;
        // set up on clicks that loads the modal
        $('.btn-book').click(function () {
            $("#modalBtn").removeAttr("disabled");
            if ($(this).hasClass('athome')) {
                $('.location').show();
                $('.postCode').show();
                $("#postCode, #locationChoice").rules("add", {
                            required: true
                });
                $("#postCode").rules("remove");
                min = 1;
                max = 500;
            } else {
                $('.location').hide();
                $('.postCode').hide();
                $("#postCode, #locationChoice").rules("remove");
               // $('.locationChoice').rules("remove");
                // traverse the DOM to the nearest values and get
                $(this).parent().siblings().each(function () {

                    if ($(this).find('.minSeats').is('.minSeats')) {
                        min = $(this).find('.minSeats').text();
                    }
                    if ($(this).find('.maxSeats').is('.maxSeats')) {
                        max = $(this).find('.maxSeats').text();
                    }
                });
            }

            // set form values and validation accordingly
            $('.modal-title').text($(this).parent().parent().find('.chefName').text() + ' Enquiry Form');
            $('.chef-price').text($(this).parent().parent().find('.price').text());
            $('.chef-loc').text($(this).parent().parent().find('.chef-location').text());
            $('#noPeople').attr({
                min: min,
                max: max
            });
            $("#noPeople").rules("add", {
                required: true,
                digits: true,
                range: [min, max]
            });
            
            validator.resetForm();
          
            //reset form values to empty
            $('#privategcform')[0].reset();
          
       
            // make sure the form will be visible in case of second submission by a visitor
            $('#privategcform').show();
           
            // make sure the success messgage from last submit is removed
            $('#successMsg').hide();
            // show the modal
            $('#evModal').modal();
        }) // END of set up on click

		// validation initiation for form
		validator = $("#privategcform").validate({
			  rules: {
			     yourName: {
			      required: true
			    },
			    yourEmail: {
			      required: true,
			      email: true
			    }
			  }
			});

		// form submission logic
		$('#privategcform').submit(function(e) {
			e.preventDefault();
			// comment out these two lines once tested
			//$('#successMsg').show();
			//$('#privategcform').hide();
			if ($('#privategcform').valid()) {
			    $("#modalBtn").attr("disabled", "disabled");
			    privateRequest();

			}
		})
		function privateRequest() {

		    
		    $('.modalBtn').attr("disabled", "disabled");
		    
			var chefName = $('.modal-title').text();
			var name = $('#yourName').val();
            var emailAddress = $('#yourEmail').val();
            var numOfguests = $('#noPeople').val();
            var dateChoice = $('#dateChoice').val();
            var postCode = $('#postCode').val();
            var locationChoice//; = $('input[name=location]:checked').val();
            var venue;
            if ($('.chef-loc').text() == null || $('.chef-loc').text() == '') {
                if (postCode == null || postCode == '') {
                    locationChoice = $('input[name=location]:checked').val()
                }
                else {
                    locationChoice = $('input[name=location]:checked').val() + "(" + postCode + ")";
                }
                venue = "Guest's Place";
            }
            else {
                locationChoice = $('.chef-loc').text();
                venue = "Chef's Venue";
            }
                        
            var price = $('.chef-price').text();
           
            var otherRequirements = $('#otherReq').val();
            var form = $('#privategcform');
            $.ajax({
                type: 'POST',
                // this url needs to be changed accordingly to the right URL - whatever the controller/routing expects
                url: '@Url.Action("PrivateGrubClubSubmit", "home")',
                data: {venue:venue, chefName : chefName, guestName: name, email : emailAddress, pricePerHead:price, numberOfGuests : numOfguests, dateChoice : dateChoice, locationChoice : locationChoice, postCode: postCode, otherRequirements : otherRequirements},
                cache: false,
                dataType: 'json text',
                traditional: true,
                success: function (result) {
                    if(result)
                    {
                        $('#privategcform').hide();

                        $('#successMsg').show();

                        form[0].reset();
                        validator.resetForm();

                        return true;
                    }
                 
                    else
                        alert('Sorry, there was an error submitting your request. Please check for errors and try again!');

                  
                },
                error: function (result) {
                    alert('Sorry, there was an error submitting your request. Please check for errors and try again!');
                }
            });
        }
       });
    </script>

