<!-- Menu Highlighter -->
<script type="text/javascript">
    var loc = window.location.pathname;
    var selectedMenuLink = $('li>a[href$="' + loc + '"]');
    var link = selectedMenuLink[0];
    if (link != null) {
        $(".toplevel>a").removeClass("current");
        if ($(link).parent().hasClass('toplevel')) {
            $(link).addClass('current');
        }
        else {
            // Assign the nearest top level menu item
            var topLink = $(link).closest('.toplevel').children(':first-child a');
            $(topLink).addClass("current");
        }
    }
</script>

