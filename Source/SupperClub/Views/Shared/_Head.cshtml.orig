    <!-- CSS
     ================================================== -->
    @if ((HttpContext.Current.Request.RequestContext.RouteData.Values["controller"].ToString().ToLower() == "admin" 
        && HttpContext.Current.Request.RequestContext.RouteData.Values["action"].ToString().ToLower() == "vouchers") 
        || (HttpContext.Current.Request.RequestContext.RouteData.Values["controller"].ToString().ToLower() == "home" 
            && (HttpContext.Current.Request.RequestContext.RouteData.Values["action"].ToString().ToLower() == "index" 
            || HttpContext.Current.Request.RequestContext.RouteData.Values["action"].ToString().ToLower() == "emailsignup")) 
        || (HttpContext.Current.Request.RequestContext.RouteData.Values["controller"].ToString().ToLower() == "host"
            && (HttpContext.Current.Request.RequestContext.RouteData.Values["action"].ToString().ToLower() == "welcome" 
            || HttpContext.Current.Request.RequestContext.RouteData.Values["action"].ToString().ToLower() == "welcomev2" 
            || HttpContext.Current.Request.RequestContext.RouteData.Values["action"].ToString().ToLower() == "welcomev3" 
            || HttpContext.Current.Request.RequestContext.RouteData.Values["action"].ToString().ToLower() == "tips"
            || HttpContext.Current.Request.RequestContext.RouteData.Values["action"].ToString().ToLower() == "details"            
            || HttpContext.Current.Request.RequestContext.RouteData.Values["action"].ToString().ToLower() == "vouchers")) 
        || (HttpContext.Current.Request.RequestContext.RouteData.Values["controller"].ToString().ToLower() == "event" 
            && (HttpContext.Current.Request.RequestContext.RouteData.Values["action"].ToString().ToLower() == "marketingtips"
            || HttpContext.Current.Request.RequestContext.RouteData.Values["action"].ToString().ToLower() == "addreview2"
            || HttpContext.Current.Request.RequestContext.RouteData.Values["action"].ToString().ToLower() == "details")) 
        || (HttpContext.Current.Request.RequestContext.RouteData.Values["controller"].ToString().ToLower() == "booking" 
            && HttpContext.Current.Request.RequestContext.RouteData.Values["action"].ToString().ToLower() == "postpaymentsuccess")
        || HttpContext.Current.Request.RequestContext.RouteData.Values["action"].ToString().ToLower() == "detailsbyname")
    {
        <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/css/newstyle.css")" />
    }    
    else
    {
        <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/css/style.css")" />
    }
